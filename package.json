{
  "name": "@hydroserver/client",
  "version": "0.0.1",
  "description": "HydroServer TypeScript client",
  "type": "module",
  "main": "dist/@hydroserver-client.umd.cjs",
  "module": "dist/@hydroserver-client.es.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "driver": "tsx driver/index.ts",
    "coverage": "vitest run --coverage --environment jsdom",
    "gen:types": "openapi-typescript ./schemas/data.openapi.json --output ./src/generated/data.types.d.ts && openapi-typescript ./schemas/auth.openapi.json --output ./src/generated/auth.types.d.ts",
    "gen:data-contracts": "tsx ./scripts/generate-contracts.data.ts",
    "gen:auth-contracts": "tsx ./scripts/generate-contracts.auth.ts",
    "gen:contracts": "npm run gen:data-contracts && npm run gen:auth-contracts",
    "gen": "npm run gen:types && npm run gen:contracts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hydroserver2/hydroserver-ts.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hydroserver2/hydroserver-ts/issues"
  },
  "homepage": "https://github.com/hydroserver2/hydroserver-ts#readme",
  "dependencies": {
    "cross-fetch": "^4.1.0",
    "jszip": "^3.10.1",
    "p-limit": "^6.2.0"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "openapi-typescript": "^7.9.1",
    "openapi-zod-client": "^1.18.3",
    "tsx": "^4.16.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  }
}
